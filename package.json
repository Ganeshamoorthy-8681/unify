{
  "name": "electron-app",
  "version": "1.0.2",
  "description": "my first app",
  "main": "main.js",
  "homepage": "https://github.com/Ganeshamoorthy-8681/electron-app/blob/main/README.md",
  "repository": "https://github.com/Ganeshamoorthy-8681/electron-app",
  "publish":{
    "owner":"github",
    "releaseType":"release"
  },
  "build": {
    "productName": "Sample-electron-app",
    "appId": "com.example.my-electron-app",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/256x256.ico",
      "files": [
        "build/**/*"
      ]
    },
    "linux": {
      "target": "deb",
      "icon": "build/256x256.png"
    }
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": {
    "name": "Ganeshamoorthy",
    "email": "ganeshamoorthy8681@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^24.1.0"
  },
  "dependencies": {
    "electron-updater": "^5.3.0"
  }
}
